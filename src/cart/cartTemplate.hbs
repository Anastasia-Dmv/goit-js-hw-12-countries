{{#each this}}
<li class="cart-dish__item">
    <span class="cart-dish__list-item">{{name}}</span>
    <div class="cart-dish__item-block">
        <span class="cart-dish__list-item">кол-во:  {{quantity}}</span>
        <span class="cart-dish__list-item">цена:   {{price}}</span>
        <button class="remove" data-id="{{id}}">❌</button>
    </div>

</li>
{{/each}}